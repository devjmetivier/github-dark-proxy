{
	"version": 2,
	"public": true,
	"alias": ["gh-dark.tobea.dev"],
	"github": {
		"autoAlias": true,
		"silent": true
	},
	"regions": ["cle1"],
	"routes": [
		{
			"src": "/dark\\.css",
			"dest": "dark.css"
		},
		{
			"src": "/.*",
			"dest": "index.js"
		}
	],
	"builds": [
		{
			"src": "index.js",
			"use": "@now/node"
		},
		{
			"src": "dark.css",
			"use": "@now/static"
		}
	]
}
